{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:react-native/all",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "new-cap": [
      "error",
      {
        "capIsNew": false
      }
    ],
    "require-jsdoc": "off",
    "react/prop-types": "off",
    "react-native/no-color-literals": "off"
  },
  "env": {
    "es6": true,
    "node": true
  }
}